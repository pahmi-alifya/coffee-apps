{
  "name": "JIWA",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean:unix": "find . -type dir -name node_modules | xargs rm -rf && yarn install",
    "reset": "yarn clean:unix && watchman watch-del-all",
    "android": "react-native run-android",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "ios": "react-native run-ios",
    "iosProductionRelease": "react-native run-ios --scheme=Production --configuration=DebugProd",
    "iosProductionDebug": "react-native run-ios --scheme=Production --configuration=ReleaseProd",
    "iosStagingRelease": "react-native run-ios --scheme=Staging --configuration=ReleaseStaging",
    "iosStagingDebug": "react-native run-ios --scheme=Staging --configuration=DebugStaging",
    "iosDevelopmentDebug": "react-native run-ios --scheme=Development --configuration=DebugDev",
    "iosDevelopmentRelease": "react-native run-ios --scheme=Development --configuration=ReleaseDev",
    "androidStagingRelease": "react-native run-android --variant=stagingRelease --appIdSuffix='staging'",
    "androidStagingDebug": "react-native run-android --variant=stagingDebug --appIdSuffix='staging'",
    "androidProductionRelease": "react-native run-android --variant=productionRelease --appIdSuffix='jiwagroup'",
    "androidProductionDebug": "react-native run-android --variant=productionDebug --appIdSuffix='jiwagroup'",
    "androidDevelopmentRelease": "react-native run-android --variant=developmentRelease --appIdSuffix='development'",
    "androidDevelopmentDebug": "react-native run-android --variant=developmentDebug --appIdSuffix='development'",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint --fix .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@faker-js/faker": "^7.5.0",
    "@gorhom/bottom-sheet": "^4",
    "@react-native-clipboard/clipboard": "^1.11.1",
    "@react-native-community/geolocation": "^3.0.2",
    "@react-navigation/drawer": "^6.5.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.0",
    "@shopify/flash-list": "^1.3.1",
    "@types/lodash": "^4.14.186",
    "currency.js": "^2.0.4",
    "date-fns": "^2.29.3",
    "i18next": "^21.10.0",
    "i18next-browser-languagedetector": "^6.1.8",
    "lodash": "^4.17.21",
    "lottie-react-native": "^5.1.4",
    "query-string": "^7.1.1",
    "react": "18.1.0",
    "react-hook-form": "^7.38.0",
    "react-i18next": "^11.18.6",
    "react-native": "0.70.1",
    "react-native-collapsible-tab-view": "^5.0.0-rc.13",
    "react-native-config": "^1.4.11",
    "react-native-date-picker": "^4.2.5",
    "react-native-device-info": "^10.3.0",
